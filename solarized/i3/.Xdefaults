urxvt.perl-ext-common:  default,matcher
urxvt*depth: 32
urxvt*cutchars:         "()*,<>[]{}|'
urxvt*termName:         rxvt-unicode
urxvt*urlLauncher:      firefox
urxvt*matcher.button:   1
urxvt*keysym.S-Prior:   ^[[5;2~
urxvt*keysym.S-Next:    ^[[6;2~

URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true
urxvt*urgentOnBell:       true
urxvt*geometry:      100x30
urxvt*highlightSelection: true
Urxvt.transparent: true
Urxvt*shading: 20
urxvt*tintColor: #000000
urxvt*font:             xft: UbuntuMono:pixelsize=12

urxvt*cursorBlink:   off
urxvt*pointerColor: #ffffff
urxvt*boldMode:      off
urxvt*colorMode:     on
urxvt*colorBDMode:   on
urxvt*colorULMode:   on
urxvt*cursorUnderLine: 1
urxvt*underlineColor: #ffffff
urxvt*scrollBar: false
URxvt.tabbed.tabbar-fg: 8
URxvt.tabbed.tabbar-bg: -1
URxvt.tabbed.tab-fg:    15
URxvt.tabbed.tab-bg: -1
Urxvt.tabbed.new-button: no
URxvt.tabbed.title: no
URxvt.bell-command: no
! colors ----------------------------------------------------------------
*background: #00141A
*foreground: #E8FFFF
*fadeColor: #002b36
*cursorColor: #93a1a1
*pointerColorBackground: #586e75
*pointerColorForeground: #93a1a1

! black
urxvt*.color0:       #000000
urxvt*.color8:       #808080

! red
urxvt*.color1:       #FF3a36
urxvt*.color9:       #FF3a36

! green
urxvt*.color2:       #DDFF00
urxvt*.color10:      #bcd900
! yellow
urxvt*.color3:       #FF5E1C
urxvt*.color11:      #FF5E1C

! blue
urxvt*.color4:       #298FD9
urxvt*.color12:      #2EA9FF

! magenta
urxvt*.color5:       #FF419D
urxvt*.color13:      #FF419D

! cyan
urxvt*.color6:       #42FFF1
urxvt*.color14:      #42FFF1

! white
urxvt*.color7:       #EEE8D5
urxvt*.color15:      #EEE8D5
! end colors ----------------------------------------------------------------

